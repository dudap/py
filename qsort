import time
def qsort(L):
    if len(L)<=1:
        return L
    pivot=L[0]
    less=[]

    for i in L:
        if pivot>i:
            less.append(i)

    equal=[]

    for i in L:
        if i==pivot:
            equal.append(i)

    greater=[]

    for i in L:
        if i>pivot:
            greater.append(i)

    return qsort(less)+equal+qsort(greater)

